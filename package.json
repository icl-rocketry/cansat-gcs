{
  "name": "Cansat-GCS",
  "productName": "ICLR CanSat GCS",
  "description": "Ground Control Station for ICLR Cansat Operations",
  "version": "0.6.0",
  "author": "Vishnu R Menon <vishthemenon@gmail.com>",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder -w --x64",
    "dist:dir": "yarn dist --dir -c.compression=store -c. .identity=null",
    "postinstall": "electron-rebuild -v 4.1.3"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": ".webpack/renderer.additions.js"
    }
  },
  "build": {
    "appId": "com.iclr.cansat-gcs",
    "directories": {
      "buildResources": "resources"
    },
    "files": [
      "static/**/**",
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "fs-jetpack": "^2.2.2",
    "serialport": "^7.1.4",
    "source-map-support": "^0.5.10",
    "typeface-open-sans": "^0.0.54"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "electron": "4.1.3",
    "electron-builder": "^20.39.0",
    "electron-webpack": "^2.6.2",
    "electron-webpack-eslint": "^4.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.11.0",
    "popper.js": "^1.15.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "three": "^0.103.0",
    "url-loader": "^1.1.2",
    "webpack": "4.28.4",
    "webpack-build-notifier": "^0.1.31"
  }
}
