.header.bg-gradient-primary.pb-8.pt-5.pt-md-8
  .container-fluid
    .header-body
      // Card stats
      .row
        .col-lg-3.col-sm-6
          .card.card-stats.mb-4.mb-xl-0
            .card-body
              .row
                .col
                  h5.card-title.text-uppercase.text-muted.mb-0 Data Frame
                  span.h2.font-weight-bold.mb-0#frame 23
                .col-auto
                  .icon.icon-shape.bg-danger.text-white.rounded-circle.shadow
                    i.fas.fa-clock
              p.mt-3.mb-0.text-muted.text-sm
                span.text-danger.mr-2
                  i.fa.fa-arrow-down
                  | 
                  span#runTime 101.2
                span.text-nowrap Running Time (s)

        .col-lg-3.col-sm-6
          .card.card-stats.mb-4.mb-xl-0
            .card-body
              .row
                .col
                  h5.card-title.text-uppercase.text-muted.mb-0 Soft State
                  span.h2.font-weight-bold.mb-0#softState 1
                .col-auto
                  .icon.icon-shape.bg-warning.text-white.rounded-circle.shadow
                    i.fas.fa-heartbeat
              p.mt-3.mb-0.text-muted.text-sm
                span.text-danger.mr-2
                  i.fas.fa-arrow-down
                  | 
                  span#battery 12.9%
                span.text-nowrap Battery Voltage

        .col-lg-3.col-sm-6
          .card.card-stats.mb-4.mb-xl-0
            .card-body
              .row
                .col
                  h5.card-title.text-uppercase.text-muted.mb-0 Altitude (m)
                  | 
                  span.h2.font-weight-bold.mb-0#altitude 56m
                .col-auto
                  .icon.icon-shape.bg-info.text-white.rounded-circle.shadow
                    i.fas.fa-parachute-box
              p.mt-3.mb-0.text-muted.text-sm
                span.text-danger.mr-2
                  i.fas.fa-arrow-down
                  | 
                  span#velocity 12.3
                span.text-nowrap Descent Velocity (m/s)
        
        .col-lg-3.col-sm-6
          .card.card-stats.mb-4.mb-xl-0
            .card-body
              .row
                .col
                  h5.card-title.text-uppercase.text-muted.mb-0 Pressure (Pa)
                  span.h2.font-weight-bold.mb-0#pressure 10149
                .col-auto
                  .icon.icon-shape.bg-info.text-white.rounded-circle.shadow
                    i.fas.fa-thermometer-three-quarters
              p.mt-3.mb-0.text-muted.text-sm
                span.text-success.mr-2
                  i.fas.fa-arrow-up
                  | 
                  span#temperature 25.3
                span.text-nowrap Temperature (C)